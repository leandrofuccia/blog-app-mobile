        options={{ title: 'Gerenciar Postagens' }}
      />
      <AdminDrawer.Screen
        name="Professores"
        component={ProfessoresScreen}
        options={{ title: 'Gerenciar UsuÃ¡rios' }}
      />
    </AdminDrawer.Navigator>
  );
}